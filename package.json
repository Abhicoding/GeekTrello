{
  "name": "geek-trello",
  "version": "1.0.0",
  "description": "Trello clone using React and Express",
  "main": "server.js",
  "scripts": {
    "start": "nodemon src/server/server.js",
    "dev": "concurrently --kill-others \"nodemon src/server/server.js\" \"npm run webpack --mode=development\"",
    "webpack": "./node_modules/.bin/webpack -w",
    "test": "mocha --compilers babel-core/register ./test/*test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Abhicoding/geek-trello.git"
  },
  "keywords": [
    "Trello",
    "React",
    "Express"
  ],
  "author": "Abhishek Shingane",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Abhicoding/geek-trello/issues"
  },
  "homepage": "https://github.com/Abhicoding/geek-trello#readme",
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "mocha": "^5.1.0",
    "node-sass": "^4.8.3",
    "nodemon": "^1.17.3",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.20.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14"
  }
}
